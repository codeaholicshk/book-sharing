<h1>Codeaholics HK - Book Sharing</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Reference url</th>
      <th>Suggested by</th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.name %></td>
        <td><%= link_to "External Link", book.reference_url %></td>
        <td><%= book.suggested_by %></td>
      </tr>
    <% end %>
  </tbody>
</table>
